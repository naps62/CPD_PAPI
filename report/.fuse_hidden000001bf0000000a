\section{Algorithm}
\begin{frame}%	begin slide
	\frametitle{Algorithm}
	%	from Wolfram: http://mathworld.wolfram.com/Convolution.html
	\begin{IEEEeqnarray}{C}
		{\forall} _{x \in N} \; a_{x}(t) = G \sum_{i=1}^{N} \frac{m_{i} | p_{x} - p{i} |}{| p_{x} - p_{i} |^{3}} \nonumber \\

		\left [ f\convolution g \right ] = \int^{t}_{0}{f(\tau)}
	\end{IEEEeqnarray}
\end{frame}%	end slide

